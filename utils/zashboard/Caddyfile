{
	auto_https off
	log log-stdout {
		output stdout
		format console {
			time_local
			time_format iso8601
		}
	}
}

(logging) {
	log {
		output stdout
		format console {
			time_local
			time_format iso8601
		}
	}
}

:{$ZASHBOARD_PORT:80} {
	import logging
	file_server
	root * /srv/zashboard
	try_files {path} /index.html
}
